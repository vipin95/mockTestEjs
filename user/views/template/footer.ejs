<!-- do active on click -->
<script src="assets/js/nextquestion.js"></script>
<script src="assets/js/checkAnswer.js"></script>
<script>
    var header = document.getElementById("sidebar-menu");
    var btns = header.getElementsByClassName("treeview");
    for (var i = 0; i < btns.length; i++) {
        btns[i].addEventListener("click", function() {
        var current = document.getElementsByClassName("active");

        current[0].className = current[0].className.replace(" active", "");
        this.className += " active";
        });
    }
</script>

<!-- add parent active on redirect -->
<script>
    var path = window.location.href;
     $('ul li a').each(function() {
      if (this.href === path) {
       $(this).parents('li').addClass(' active');
      }
     });
</script>